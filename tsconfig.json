{
    "compilerOptions": {
        "lib": ["esnext"],
        "strict": true,
        "rootDir": "./",
        "types": [
            "lua-types/5.3",
            "typescript-to-lua/language-extensions"
        ],
        "typeRoots": ["src"],
        "allowJs": true,
        "noImplicitAny": true
    },
    "tstl": {
        "luaTarget": "5.3",
        "luaLibImport": "require",
        "noImplicitSelf": true
    }
}
